{
  "name": "legalease-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for LegalEase AI",
  "main": "dist/index.js",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@google-cloud/documentai": "^8.5.0",
    "@google-cloud/storage": "^7.5.0",
    "@google-cloud/text-to-speech": "^5.1.0",
    "@google-cloud/translate": "^8.0.2",
    "@google-cloud/aiplatform": "^3.7.0",
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.4.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.10.2",
    "moment": "^2.29.4",
    "nanoid": "^3.3.6",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.13.0",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "@legalease/types": "workspace:*",
    "@legalease/utils": "workspace:*",
    "@legalease/config": "workspace:*"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.6",
    "@types/nodemailer": "^6.4.11",
    "@types/pdfkit": "^0.12.12",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "firebase-functions-test": "^3.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "private": true
}